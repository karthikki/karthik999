#include<stdio.h>
#include<conio.h>

 void main()
 {
   int a[5];
   int i,f;
   int count=0;

    for(i=0;i<5;i++)
    {
    printf("enter a number: ");
    scanf("%d",&a[i]);
    }

    printf("which no you want to find: ");
    scanf("%d",&f);
    for(i=0;i<5;i++)
       {
           if(a[i]==f){
            count++;
           }
       }
      printf("%d",count);

 }
